<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".mainVolunteer">


    <Button
        android:id="@+id/button20"
        android:layout_width="165dp"
        android:layout_height="87dp"
        android:layout_marginTop="436dp"
        android:backgroundTint="#c41230"
        android:text="work schedule"
        android:textColor="#263D90"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.178"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button88"
        android:layout_width="168dp"
        android:layout_height="0dp"
        android:layout_marginTop="439dp"
        android:layout_marginBottom="44dp"
        android:backgroundTint="#c41230"
        android:text="assibment to shifts"
        android:textColor="#263D90"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/job"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.88"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <ImageView
        android:id="@+id/logo"
        android:layout_width="206dp"
        android:layout_height="312dp"
        android:layout_marginTop="44dp"
        android:src="@drawable/download"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/job"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="80dp"
        android:layout_marginBottom="120dp"
        android:text="Search a job"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/button88"
        app:layout_constraintTop_toBottomOf="@+id/button88" />

</androidx.constraintlayout.widget.ConstraintLayout>